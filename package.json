{
  "name": "NoteTaking",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "install-all": "yarn install --legacy-peer-deps && cd ios && pod install --verbose && cd ..",
    "install-new": "yarn install --legacy-peer-deps && cd ios && pod install --repo-update --verbose && cd ..",
    "install-pods": "cd ios && pod install --verbose && cd ..",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "update-pods": "cd ios && pod repo update && pod update && cd ..",
    "clean:android:gradle": "rm -rf android/.gradle && rm -rf .gradle &&: rm -rf ~/.gradle && cd android && ./gradlew clean && cd ..",
    "clean:ios:cocopods": "rm -rf ~/Library/Caches/CocoaPods && rm -rf ios/Pods",
    "post-install": "react-native setup-ios-permissions && cd ios && pod install --verbose && cd ..",
    "build-apk": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "android-release": "react-native run-android --mode=release",
    "android-logs": "react-native log-android",
    "ios-logs": "react-native log-ios",
    "build-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.1.0",
    "react": "18.2.0",
    "react-native": "0.73.3",
    "react-native-cn-quill": "^0.7.18",
    "react-native-mmkv": "^2.11.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-webview": "^11.0.0",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.20",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.4",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
